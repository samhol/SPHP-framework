{
  "name": "sami_holck/sphp_framework",
  "homepage": "http://www.playground.samiholck.com/",
  "description": "Multipurpose PHP framework for web applications",
  "keywords": ["PHP", "HTML5", "WEB 2.0", "UI"],
  "license": "MIT",
  "authors": [
    {
      "name": "Sami Holck",
      "email": "sami.holck@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "doctrine/orm": ">=2.5",
    "sepia/po-parser": ">=5",
    "imagine/imagine": ">=0.7",
    "erusev/parsedown-extra": "~0.7",
    "tovic/parsedown-extra-plugin": "1.1.6",
    "geshi/geshi": ">=1",
    "jdorn/sql-formatter": ">=1.2",
    "gajus/dindent": ">=2.0",
    "zendframework/zend-cache": "*",
    "zendframework/zend-config": "*",
    "zendframework/zend-i18n": "*",
    "zendframework/zend-paginator": "*",
    "zendframework/zend-mail": "*",
    "symfony/yaml": ">=3.0",
    "mpdf/mpdf": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8",
    "phpunit/php-code-coverage": "*",
    "mockery/mockery": "@stable",
    "eloquent/phony-phpunit": "*",
    "mikey179/vfsstream": "~1"
  },
  "autoload": {
    "psr-4": {
      "Sphp\\": "sphp/php/Sphp",
      "Sphp\\Manual\\": "manual/classes"
    },
    "psr-0": {
      "": "vendor/tovic/parsedown-extra-plugin"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sphp\\Tests\\": "sphp/php/tests/sphp"
    }
  }
}
