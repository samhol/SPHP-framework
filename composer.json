{
  "name": "sami_holck/sphp_framework",
  "homepage": "http://www.playground.samiholck.com/",
  "description": "Multipurpose PHP framework for web applications",
  "keywords": ["HTML5", "WEB 2.0", "UI"],
  "license": "GNU",
  "authors": [
    {
      "name": "Sami Holck",
      "email": "sami.holck@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "tovic/parsedown-extra-plugin",
        "version": "1.0",
        "source": {
          "url": "https://github.com/tovic/parsedown-extra-plugin",
          "type": "git",
          "reference": "origin/master"
        }
      }
    }
  ],
  "require": {
    "php": ">=7.0",
    "doctrine/orm": ">=2.5",
    "sepia/po-parser": ">=4.2",
    "imagine/imagine": ">=0.7",
    "erusev/parsedown-extra": ">=0.7.0",
    "tovic/parsedown-extra-plugin": "*",
    "geshi/geshi": "^1",
    "jdorn/sql-formatter": ">=1.2",
    "gajus/dindent": ">=2.0",
    "zendframework/zend-cache": "*",
    "zendframework/zend-config": "*",
    "zendframework/zend-i18n": "*",
    "zendframework/zend-paginator": "*",
    "zendframework/zend-mail": "*",
    "zurb/foundation": ">=6.4",
    "symfony/yaml": ">=3.0",
    "tecnickcom/TCPDF": "*"
  },
  "autoload": {
    "psr-4": {
      "Sphp\\": "sphp/php/Sphp",
      "Sphp\\Manual\\": "manual/classes"
    },
    "psr-0": {
      "": "vendor/tovic/parsedown-extra-plugin"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sphp\\Tests\\": "tests/sphp"
    }
  }
}
